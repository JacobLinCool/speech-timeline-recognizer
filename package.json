    "files": [
        "dist"
    ],
    "main": "dist/str.js",
    "types": "dist/str.d.ts",
    "bin": {
        "str": "dist/str.js"
    },
    "scripts": {
        "start": "node dist/str.js",
        "test": "jest --coverage",
        "dev": "tsup --watch",
        "build": "tsup",
        "docker": "docker build -t str -t jacoblincool/str .",
        "docker:push": "docker buildx build --push --platform linux/arm64/v8,linux/amd64 -t jacoblincool/str .",
        "docs": "typedoc ./src/",
        "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
        "lint": "eslint .",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "dependencies": {
        "commander": "^9.4.0",
        "fuse.js": "^6.6.2",
        "opencc": "^1.1.3",
        "vosk": "0.3.39",
        "wav": "^1.0.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.3",
        "@types/wav": "^1.0.1",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.3",
        "prettier": "^2.7.1",
